      </addressBlock>

      <registers>
        <register>
          <name>TIFR</name>
          <description>Timer Interrupt Flag Register</description>
          <addressOffset>0x0</addressOffset>

          <fields>
            <field>
              <name>OCF_B</name>
              <description>Output Compare B Match Flag</description>
              <bitRange>[2:2]</bitRange>
            </field>
            <field>
              <name>OCF_A</name>
              <description>Output Compare A Match Flag</description>
              <bitRange>[1:1]</bitRange>
            </field>
