        <register>
          <name>TCCR_A</name>
          <description>Timer/Counter Control Register A</description>
          <addressOffset>0xf</addressOffset>

          <fields>
            <field>
              <name>COM_A</name>
              <description>Compare Match Output A Mode</description>
              <bitRange>[7:6]</bitRange>
              <writeConstraint>
                <useEnumeratedValues>true</useEnumeratedValues>
              </writeConstraint>
                        

              <enumeratedValues>
                <enumeratedValue>
                  <name>DISCONNECTED</name>
                  <description>Normal port operation, OC0x disconnected</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>MATCH_TOGGLE</name>
                  <description>Toggle OC0x on Compare Match (Might depend on WGM)</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>MATCH_CLEAR</name>
                  <description>Clear OC0x on Compare Match (If PWM is enabled, OC0x is set at TOP)</description>
                  <value>2</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>MATCH_SET</name>
                  <description>Set OC0x on Compare Match (If PWM is enabled, OC0x is cleared at TOP)</description>
                  <value>3</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>COM_B</name>
              <description>Compare Match Output B Mode</description>
              <bitRange>[5:4]</bitRange>
              <writeConstraint>
